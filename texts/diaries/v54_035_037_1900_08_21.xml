     <choice>
      <reg>
       Бог
      </reg>
      <orig>
       Богъ
      </orig>
     </choice>
     <choice>
      <reg>
       даст
      </reg>
      <orig>
       дастъ
      </orig>
     </choice>
     .
     <ref target="#note173">
