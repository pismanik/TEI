    <p id="3ee7afa0-25f3-43f7-8337-571ce9d4dba5">
     Мало
     <choice>
      <reg>
       спал
      </reg>
      <orig>
       спалъ
      </orig>
     </choice>
     . И умственно
     <choice>
      <reg>
       бодр
      </reg>
      <orig>
       бодръ
      </orig>
     </choice>
     .
     <choice>
      <reg>
       Приехал
      </reg>
      <orig>
       Прiѣхалъ
      </orig>
     </choice>
     <choice>
      <reg>
       Наживин
      </reg>
      <orig>
       Наживинъ
      </orig>
     </choice>
     .
     <choice>
      <reg>
       Он
      </reg>
      <orig>
       Онъ
      </orig>
     </choice>
     <choice>
      <reg>
       мне
      </reg>
      <orig>
       мнѣ
      </orig>
     </choice>
     <choice>
      <reg>
       приятен
      </reg>
      <orig>
       пріятенъ
      </orig>
     </choice>
     .
     <choice original_editorial_correction="M[apья]">
      <sic>
       M
      </sic>
      <corr>
       Марья
      </corr>
     </choice>
     <choice original_editorial_correction="А[лександровна]">
      <sic>
       А
      </sic>
      <corr>
       <name placement="text" ref="15706" type="person">
        Александровна
       </name>
      </corr>
     </choice>
     и
     <choice original_editorial_correction="Бул[анже]">
      <sic>
       Бул
      </sic>
      <corr>
       Буланже
      </corr>
     </choice>
     . Письма
     <choice original_editorial_correction="неинтересн[ыя]">
      <sic>
       неинтересн
      </sic>
      <corr>
       <choice>
        <reg>
         неинтересные
        </reg>
        <orig>
         неинтересныя
        </orig>
       </choice>
      </corr>
     </choice>
     .
     <choice>
      <reg>
       Поправил
      </reg>
      <orig>
       Поправилъ
      </orig>
     </choice>
     2 и 3 день.
     <choice>
      <reg>
       Ездил
      </reg>
      <orig>
       Ѣздилъ
      </orig>
     </choice>
     немного
     <choice original_editorial_correction="верх[омъ]">
      <sic>
       верх
      </sic>
      <corr>
       <choice>
        <reg>
         верхом
        </reg>
        <orig>
         верхомъ
        </orig>
       </choice>
      </corr>
     </choice>
     .
     <choice>
      <reg>
       Согрешил
      </reg>
      <orig>
       Согрѣшилъ
      </orig>
     </choice>
     <choice>
      <reg>
       с
      </reg>
      <orig>
       съ
      </orig>
     </choice>
     бабой издалека —
     <choice>
      <reg>
       отказал
      </reg>
      <orig>
       отказалъ
      </orig>
     </choice>
     .
     <choice>
      <reg>
       Обед
      </reg>
      <orig>
       Обѣдъ
      </orig>
     </choice>
     и
     <choice>
      <reg>
       вечер
      </reg>
      <orig>
       вечеръ
      </orig>
     </choice>
     <choice>
      <reg>
       приятно
      </reg>
      <orig>
       пріятно
      </orig>
     </choice>
     и
     <choice original_editorial_correction="дѣльн[о]">
      <sic>
       дельн
      </sic>
      <corr>
       дельно
      </corr>
     </choice>
     , и
     <choice>
      <reg>
       с
      </reg>
      <orig>
       съ
      </orig>
     </choice>
     <choice original_editorial_correction="Нажив[инымъ]">
      <sic>
       Нажив
      </sic>
      <corr>
       <choice>
        <reg>
         Наживиным
        </reg>
        <orig>
         Наживинымъ
        </orig>
       </choice>
      </corr>
     </choice>
     , и
     <choice>
      <reg>
       с
      </reg>
      <orig>
       съ
      </orig>
     </choice>
     Буланже.
     <choice original_editorial_correction="Булан[же]">
      <sic>
       Булан
      </sic>
      <corr>
       Буланже
      </corr>
     </choice>
     о
     <choice>
      <reg>
       Будде
      </reg>
      <orig>
       Буддѣ
      </orig>
     </choice>
     прекрасно. Записать что то есть, но до завтра.
     <choice original_editorial_correction="С[аша]">
      <sic>
       С
      </sic>
      <corr>
       <name placement="text" ref="13815" type="person">
        Саша
       </name>
      </corr>
     </choice>
     <choice>
      <reg>
       с
      </reg>
      <orig>
       съ
      </orig>
     </choice>
     <choice original_editorial_correction="Душ[аномъ]">
      <sic>
       Душ
      </sic>
      <corr>
       <choice>
        <reg>
         <name placement="text" ref="8365" type="person">
          Душаном
         </name>
        </reg>
        <orig>
         Душаномъ
        </orig>
       </choice>
      </corr>
     </choice>
     <choice>
      <reg>
       в
      </reg>
      <orig>
       въ
      </orig>
     </choice>
     <choice>
      <reg>
       Туле
      </reg>
      <orig>
       Тулѣ
      </orig>
     </choice>
     на
     <choice>
      <reg>
       концерте
      </reg>
      <orig>
       концертѣ
      </orig>
     </choice>
     .
