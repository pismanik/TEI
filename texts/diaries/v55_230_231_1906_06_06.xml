      </orig>
     </choice>
     <choice>
      <reg>
       вопрос
      </reg>
      <orig>
       вопросъ
      </orig>
     </choice>
     , надо
     <choice>
      <reg>
       решать
      </reg>
      <orig>
       рѣшать
      </orig>
     </choice>
     его не
     <choice>
      <reg>
       в
      </reg>
      <orig>
       въ
      </orig>
     </choice>
     виду
     <choice>
      <reg>
       наших
      </reg>
      <orig>
       нашихъ
      </orig>
     </choice>
     <choice>
      <reg>
       представлений
      </reg>
      <orig>
       представленій
      </orig>
