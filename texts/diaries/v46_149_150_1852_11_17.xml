     <choice>
      <reg>
       раз
      </reg>
      <orig>
       разъ
      </orig>
     </choice>
     <choice>
      <reg>
       писал
      </reg>
      <orig>
       писалъ
      </orig>
     </choice>
     <choice original_editorial_correction="п[исьма]">
      <sic>
       п
      </sic>
      <corr>
       письма
      </corr>
     </choice>
     <choice original_editorial_correction="Д[ьякову]">
      <sic>
       Д
      </sic>
      <corr>
       <name placement="text" ref="4739" type="person">
        Дьякову
       </name>
      </corr>
     </choice>
     и
     <choice original_editorial_correction="Р[едактору]">
      <sic>
       Р
      </sic>
      <corr>
       Редактору
      </corr>
     </choice>
     ,
     <choice original_editorial_correction="к[оторыя]">
      <sic>
       к
      </sic>
      <corr>
       <choice>
        <reg>
         которые
        </reg>
        <orig>
         которыя
        </orig>
       </choice>
      </corr>
     </choice>
