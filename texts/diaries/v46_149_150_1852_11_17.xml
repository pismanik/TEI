     <choice original_editorial_correction="Р[едактору]">
      <sic>
       Р
      </sic>
      <corr>
       Редактору
      </corr>
     </choice>
     <choice>
      <reg>
       слишком
      </reg>
      <orig>
       слишкомъ
      </orig>
     </choice>
