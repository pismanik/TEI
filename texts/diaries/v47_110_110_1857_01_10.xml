     <choice>
      <reg>
       с
      </reg>
      <orig>
       съ
      </orig>
     </choice>
     <choice original_editorial_correction="К[изиветеромъ]">
      <sic>
       К
      </sic>
      <corr>
       <choice>
        <reg>
         Кизиветером
        </reg>
        <orig>
         Кизиветеромъ
        </orig>
       </choice>
      </corr>
     </choice>
     .
     <choice>
      <reg>
       Кизиветер
      </reg>
      <orig>
       Кизиветеръ
      </orig>
     </choice>
     <hi>
      глубоко
