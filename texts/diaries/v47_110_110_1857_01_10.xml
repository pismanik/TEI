     <choice>
      <reg>
       решился
      </reg>
      <orig>
       рѣшился
      </orig>
     </choice>
     <choice>
      <reg>
       ехать
      </reg>
      <orig>
       ѣхать
      </orig>
     </choice>
     .
     <choice>
      <reg>
       Взял
      </reg>
      <orig>
       Взялъ
      </orig>
     </choice>
     <choice>
      <reg>
       денег
      </reg>
      <orig>
       денегъ
      </orig>
     </choice>
     у
     <name placement="text" ref="10447" type="person">
      Панаева
     </name>
     .
     <choice>
      <reg>
       Обедали
      </reg>
      <orig>
       Обѣдали
      </orig>
     </choice>
     у меня
     <choice original_editorial_correction="Брем[еръ]">
      <sic>
