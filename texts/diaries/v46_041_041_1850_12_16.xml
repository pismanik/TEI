     <name placement="text" ref="8143" type="person">
      Львову
     </name>
     ,
     <choice>
      <reg>
       обедать
      </reg>
      <orig>
       обѣдать
      </orig>
     </choice>
     дома и
     <choice>
      <reg>
       вечером
      </reg>
      <orig>
       вечеромъ
      </orig>
     </choice>
