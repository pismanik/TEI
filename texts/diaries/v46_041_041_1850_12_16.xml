     <choice>
      <reg>
       в
      </reg>
      <orig>
       въ
      </orig>
     </choice>
     баню, послать
     <choice>
      <reg>
       в
      </reg>
      <orig>
       въ
      </orig>
     </choice>
     <choice>
      <reg>
       совет
      </reg>
      <orig>
       совѣтъ
      </orig>
     </choice>
