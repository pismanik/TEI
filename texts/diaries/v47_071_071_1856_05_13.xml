     <choice>
      <reg>
       он
      </reg>
      <orig>
       онъ
      </orig>
     </choice>
     <choice>
      <reg>
       перебирает
      </reg>
      <orig>
       перебираетъ
      </orig>
     </choice>
     <choice>
      <reg>
       все
      </reg>
      <orig>
       всѣ
      </orig>
     </choice>
