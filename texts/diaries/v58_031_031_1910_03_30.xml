    <p id="c5caebb5-2a82-4bcc-a3e3-28fb491d4825">
     <choice>
      <reg>
       Пропустил
      </reg>
      <orig>
       Пропустилъ
      </orig>
     </choice>
     два дня.
     <hi>
      Вчера 29.
     </hi>
     <choice>
      <reg>
       Утром
      </reg>
      <orig>
       Утромъ
      </orig>
     </choice>
     <ref target="#note84">
      84
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note84">
      <hi>
       Зачеркнуто
      </hi>
      : по
      <choice original_editorial_correction="доро[гѣ]">
       <sic>
        доро
       </sic>
       <corr>
        <choice>
         <reg>
          дороге
         </reg>
         <orig>
          дорогѣ
         </orig>
        </choice>
       </corr>
      </choice>
