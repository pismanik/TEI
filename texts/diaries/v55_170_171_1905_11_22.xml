     <choice>
      <reg>
       соображениями
      </reg>
      <orig>
       соображеніями
      </orig>
     </choice>
     о
     <choice>
      <reg>
       нем
      </reg>
      <orig>
       немъ
      </orig>
     </choice>
     ,
     <choice>
      <reg>
       вернее
      </reg>
      <orig>
       вѣрнѣе
      </orig>
     </choice>
     <choice>
      <reg>
       решать
      </reg>
      <orig>
       рѣшать
      </orig>
     </choice>
     поступки
     <choice>
      <reg>
       настоящего
      </reg>
      <orig>
       настоящаго
      </orig>
     </choice>
     , а
     <choice>
      <reg>
       никак
      </reg>
      <orig>
       никакъ
      </orig>
     </choice>
