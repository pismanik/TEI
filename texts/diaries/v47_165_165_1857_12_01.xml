     <choice>
      <reg>
       потом
      </reg>
      <orig>
       потомъ
      </orig>
     </choice>
     <choice>
      <reg>
       убил
      </reg>
      <orig>
       убилъ
      </orig>
     </choice>
     скукой.
     <choice>
      <reg>
