       <choice>
        <reg>
         <name placement="text" ref="6393" type="person">
          Александрин
         </name>
        </reg>
        <orig>
         Александринъ
        </orig>
       </choice>
      </corr>
     </choice>
     <choice>
      <reg>
       держит
      </reg>
      <orig>
       держитъ
      </orig>
     </choice>
