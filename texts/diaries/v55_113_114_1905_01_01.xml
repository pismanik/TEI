     <choice original_editorial_correction="к[оторыхъ]">
      <sic>
       к
      </sic>
      <corr>
       <choice>
        <reg>
         которых
        </reg>
        <orig>
         которыхъ
        </orig>
       </choice>
      </corr>
     </choice>
     <choice>
      <reg>
       нам
      </reg>
      <orig>
       намъ
      </orig>
     </choice>
     бы было хорошо, а между
     <choice>
      <reg>
       тем
      </reg>
      <orig>
       тѣмъ
      </orig>
     </choice>
     <choice>
      <reg>
       нам
      </reg>
      <orig>
       намъ
      </orig>
     </choice>
     хорошо
     <choice>
      <reg>
       может
      </reg>
      <orig>
       можетъ
      </orig>
     </choice>
     быть только
     <choice>
      <reg>
       от
      </reg>
      <orig>
       отъ
      </orig>
     </choice>
     нашего
     <choice>
      <reg>
       усилия
      </reg>
      <orig>
       усилія
      </orig>
     </choice>
     побороть то, что
     <choice>
      <reg>
       нам
      </reg>
      <orig>
       намъ
      </orig>
     </choice>
     нехорошо.
     <choice>
      <reg>
       Так
      </reg>
      <orig>
       Такъ
      </orig>
     </choice>
