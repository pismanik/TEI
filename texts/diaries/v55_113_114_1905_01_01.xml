     <choice>
      <reg>
       ослабляет
      </reg>
      <orig>
       ослабляетъ
      </orig>
     </choice>
     нашу
     <choice>
      <reg>
       энергию
      </reg>
      <orig>
       энергію
      </orig>
     </choice>
