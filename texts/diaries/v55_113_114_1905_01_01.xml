     <ref target="#note512">
      512
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note512">
      <hi>
       Зачеркнуто:
      </hi>
      <choice original_editorial_correction="осл[абляетъ]">
       <sic>
        осл
       </sic>
       <corr>
        <choice>
         <reg>
          ослабляет
