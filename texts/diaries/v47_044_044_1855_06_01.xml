     <choice>
      <reg>
       гадал
      </reg>
      <orig>
       гадалъ
      </orig>
     </choice>
     наи ничего не
     <choice>
      <reg>
       делал
      </reg>
      <orig>
       дѣлалъ
      </orig>
     </choice>
     .
     <choice>
      <reg>
       Вечер
      </reg>
      <orig>
       Вечеръ
      </orig>
     </choice>
     <choice>
      <reg>
       сидел
      </reg>
      <orig>
       сидѣлъ
      </orig>
     </choice>
