     <choice>
      <reg>
       лень
      </reg>
      <orig>
       лѣнь
      </orig>
     </choice>
     и
     <choice>
      <reg>
       бесхарактерность
      </reg>
      <orig>
       безхарактерность
      </orig>
     </choice>
