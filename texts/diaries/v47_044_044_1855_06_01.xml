     <choice>
      <reg>
       от
      </reg>
      <orig>
       отъ
      </orig>
     </choice>
     <choice>
      <reg>
       ранок
      </reg>
      <orig>
       ранокъ
      </orig>
     </choice>
     ,
     <choice>
      <reg>
       которые
      </reg>
      <orig>
       которыя
      </orig>
     </choice>
