     <choice>
      <reg>
       от
      </reg>
      <orig>
       отъ
      </orig>
     </choice>
     <ref target="#note76">
      76
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note76">
      <hi>
       Зачеркнуто:
      </hi>
      <choice>
       <reg>
        мух
       </reg>
       <orig>
        мухъ
       </orig>
      </choice>
     </note>
     <choice>
      <reg>
       муравьев
      </reg>
      <orig>
       муравьевъ
      </orig>
     </choice>
