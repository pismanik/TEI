     <choice>
      <reg>
       Вечером
      </reg>
      <orig>
       Вечеромъ
      </orig>
     </choice>
     <choice>
      <reg>
       пришел
      </reg>
      <orig>
       пришелъ
      </orig>
     </choice>
     <name placement="text" ref="4633" type="person">
      Саша
