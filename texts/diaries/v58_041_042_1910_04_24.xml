     <choice>
      <reg>
       маразм
      </reg>
      <orig>
       маразмъ
      </orig>
     </choice>
     .
     <choice>
      <reg>
       От
      </reg>
      <orig>
       Отъ
      </orig>
     </choice>
     <hi style="opdelimiter">
