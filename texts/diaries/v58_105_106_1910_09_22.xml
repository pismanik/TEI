     <choice>
      <reg>
       Жалел
      </reg>
      <orig>
       Жалѣлъ
      </orig>
     </choice>
     ,
     <choice original_editorial_correction="ч[то]">
      <sic>
       ч
      </sic>
      <corr>
       что
      </corr>
     </choice>
     не
     <choice>
      <reg>
       заехал
      </reg>
      <orig>
       заѣхалъ
      </orig>
     </choice>
     <choice>
      <reg>
       в
      </reg>
      <orig>
       въ
      </orig>
     </choice>
     школу Горбова.
     <choice>
      <reg>
       Он
      </reg>
      <orig>
       Онъ
      </orig>
     </choice>
     <choice>
      <reg>
       вышел
      </reg>
      <orig>
       вышелъ
      </orig>
     </choice>
     <choice>
      <reg>
