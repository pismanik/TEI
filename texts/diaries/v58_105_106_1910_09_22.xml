      </sic>
      <corr>
       <choice>
        <reg>
         <name placement="text" ref="51" type="person">
          Абрикосовым
         </name>
        </reg>
        <orig>
         Абрикосовымъ
        </orig>
       </choice>
      </corr>
     </choice>
