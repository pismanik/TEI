     <choice>
      <reg>
       как
      </reg>
      <orig>
       какъ
      </orig>
     </choice>
     <choice>
      <reg>
       в
      </reg>
      <orig>
       въ
      </orig>
     </choice>
     <choice>
      <reg>
       деревне
      </reg>
      <orig>
       деревнѣ
      </orig>
     </choice>
     ,
     <choice>
      <reg>
       в
      </reg>
      <orig>
       въ
      </orig>
     </choice>
     <choice>
      <reg>
       помещичьей
      </reg>
      <orig>
       помѣщичьей
      </orig>
     </choice>
     <choice>
      <reg>
       усадьбе
      </reg>
      <orig>
       усадьбѣ
      </orig>
     </choice>
     не видна
     <choice>
      <reg>
       так
      </reg>
      <orig>
       такъ
      </orig>
     </choice>
     ясно вся
     <choice>
      <reg>
