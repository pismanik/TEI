     <choice>
      <reg>
       Заезжали
      </reg>
      <orig>
       Заѣзжали
      </orig>
     </choice>
     <choice>
      <reg>
       к
      </reg>
      <orig>
       къ
      </orig>
     </choice>
     <choice>
