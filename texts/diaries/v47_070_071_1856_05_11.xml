    <p id="abeb3923-f75c-481e-8e4f-7dbdcdf9db66">
     Вчера
     <choice>
      <reg>
       утром
      </reg>
      <orig>
       утромъ
      </orig>
     </choice>
     <choice>
      <reg>
       писал
      </reg>
      <orig>
       писалъ
      </orig>
     </choice>
     письмо
     <choice original_editorial_correction="Т[атьянѣ]">
      <sic>
       Т
      </sic>
      <corr>
       <choice>
        <reg>
         <name placement="text" ref="4947" type="person">
          Татьяне
         </name>
        </reg>
        <orig>
         <name placement="text" ref="4947" type="person">
          Татьянѣ
         </name>
        </orig>
       </choice>
      </corr>
     </choice>
     <choice original_editorial_correction="А[лександровнѣ]">
      <sic>
       <name placement="text" ref="4947" type="person">
