     <ref target="#note295">
      295
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note295">
      <hi>
       Зачеркнуто:
      </hi>
      выкинула
     </note>
     <choice>
      <reg>
       в
      </reg>
      <orig>
       въ
      </orig>
     </choice>
     моей
     <choice>
      <reg>
       комнате
      </reg>
      <orig>
       комнатѣ
      </orig>
     </choice>
     мои
     <choice original_editorial_correction="портре[ты]">
      <sic>
       портре
      </sic>
      <corr>
       портреты
