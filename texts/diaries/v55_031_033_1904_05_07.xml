     хорошо разработанную землю
     <ref target="#note141">
      141
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note141">
      <hi>
       Зачеркнуто:
      </hi>
      кинуть
      <choice>
       <reg>
        в
       </reg>
       <orig>
        въ
       </orig>
      </choice>
      нее
     </note>
     попали
     <ref target="#note142">
      142
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note142">
      <hi>
       Зачеркнуто:
      </hi>
      <choice>
       <reg>
        овес
       </reg>
       <orig>
        овесъ
       </orig>
      </choice>
     </note>
     <choice>
      <reg>
       семена
      </reg>
      <orig>
       сѣмена
      </orig>
     </choice>
     и при
     <choice>
      <reg>
       этом
      </reg>
      <orig>
       этомъ
      </orig>
     </choice>
     <choice>
      <reg>
       будет
      </reg>
      <orig>
       будетъ
      </orig>
     </choice>
     тепло, влага, и ничто не
     <choice>
      <reg>
       затопчет
      </reg>
      <orig>
       затопчетъ
      </orig>
     </choice>
     <choice>
      <reg>
       посев
      </reg>
      <orig>
       посѣвъ
      </orig>
     </choice>
     , то
     <ref target="#note143">
      143
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note143">
      <hi>
       Зачеркнуто:
      </hi>
      <choice>
       <reg>
        будет
       </reg>
       <orig>
        будетъ
       </orig>
      </choice>
     </note>
     <choice>
      <reg>
       вырастут
      </reg>
      <orig>
       вырастутъ
      </orig>
     </choice>
     <ref target="#note144">
      144
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note144">
      <hi>
       Зачеркнуто:
      </hi>
      <choice>
       <reg>
        овес
       </reg>
       <orig>
        овесъ
       </orig>
      </choice>
      .
     </note>
     <choice>
      <reg>
       известные
      </reg>
      <orig>
       извѣстныя
      </orig>
     </choice>
     <choice>
      <reg>
       растения
      </reg>
      <orig>
       растенія
      </orig>
     </choice>
     . Возможно также
     <choice>
      <reg>
       верно
      </reg>
      <orig>
       вѣрно
      </orig>
     </choice>
     <choice>
      <reg>
       определить
      </reg>
      <orig>
       опредѣлить
      </orig>
     </choice>
     ,
     <choice>
      <reg>
       какие
      </reg>
      <orig>
       какія
      </orig>
     </choice>
     <choice>
      <reg>
       будут
      </reg>
      <orig>
       будутъ
      </orig>
     </choice>
     <ref target="#note145">
      145
     </ref>
     <note exclude="true" index="false" resp="volume_editor" xml:id="note145">
      <hi>
       Зачеркнуто:
      </hi>
      умственныя
     </note>
     <choice>
      <reg>
       духовные
      </reg>
      <orig>
       духовныя
      </orig>
     </choice>
     <choice>
      <reg>
       последствия
      </reg>
      <orig>
       послѣдствія
      </orig>
     </choice>
     <choice>
      <reg>
       известных
      </reg>
      <orig>
       извѣстныхъ
      </orig>
     </choice>
     <choice>
      <reg>
       умственных
      </reg>
      <orig>
       умственныхъ
      </orig>
     </choice>
     ,
     <choice original_editorial_correction="художеств[енныхъ]">
      <sic>
       художеств
