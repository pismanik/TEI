     <choice>
      <reg>
       Впрочем
      </reg>
      <orig>
       Впрочемъ
      </orig>
     </choice>
     <choice>
      <reg>
       вспомнил
      </reg>
      <orig>
       вспомнилъ
      </orig>
     </choice>
     .
     <ref target="#note140">
