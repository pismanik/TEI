     <choice>
      <reg>
       стесняет
      </reg>
      <orig>
       стѣсняетъ
      </orig>
     </choice>
     <choice>
      <reg>
       их
      </reg>
      <orig>
       ихъ
      </orig>
     </choice>
     <choice>
      <reg>
       более
      </reg>
      <orig>
