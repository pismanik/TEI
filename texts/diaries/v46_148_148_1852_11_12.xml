    <p id="8a7f5dbc-5120-48cb-895e-33c4d695df9d">
     Утро
     <choice>
      <reg>
       ходил
      </reg>
      <orig>
       ходилъ
      </orig>
     </choice>
     <choice>
      <reg>
       к
      </reg>
      <orig>
       къ
      </orig>
     </choice>
     <choice>
      <reg>
       Иляске
      </reg>
      <orig>
       Иляскѣ
      </orig>
     </choice>
     и
     <name placement="text" ref="14899" type="person">
      Хилковскому
     </name>
     .
     <choice>
      <reg>
       Ездил
      </reg>
      <orig>
       Ѣздилъ
      </orig>
     </choice>
     <choice>
      <reg>
       с
      </reg>
      <orig>
       съ
      </orig>
     </choice>
     собаками,
     <choice>
      <reg>
       убил
      </reg>
      <orig>
       убилъ
      </orig>
     </choice>
     4
     <choice original_editorial_correction="фаз[ановъ]">
      <sic>
       фаз
      </sic>
      <corr>
       <choice>
        <reg>
         фазанов
        </reg>
        <orig>
         фазановъ
        </orig>
       </choice>
      </corr>
     </choice>
     и утку.
     <choice>
      <reg>
       Вечером
      </reg>
      <orig>
       Вечеромъ
      </orig>
     </choice>
     было очень грустно.
     <choice>
      <reg>
       Написал
      </reg>
      <orig>
       Написалъ
      </orig>
     </choice>
     письмо
     <name placement="text" ref="4739" type="person">
      Дьякову
     </name>
     ,
     <choice original_editorial_correction="кот[орое]">
      <sic>
       кот
      </sic>
      <corr>
       которое
      </corr>
     </choice>
     тоже не
     <hi style="opdelimiter">
      пошлю.
     </hi>
    </p>
