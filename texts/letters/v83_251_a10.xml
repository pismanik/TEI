      <reg>
       Ясенков
      </reg>
      <orig>
       Ясенковъ
      </orig>
     </choice>
     —
     <choice>
      <reg>
       интересные
      </reg>
      <orig>
       интересныя
      </orig>
     </choice>
     <choice>
      <reg>
       от
      </reg>
      <orig>
       отъ
      </orig>
     </choice>
     Sandoz
     <ref target="#note2">
      2
     </ref>
     <note index="false" resp="volume_editor" xml:id="note2">
