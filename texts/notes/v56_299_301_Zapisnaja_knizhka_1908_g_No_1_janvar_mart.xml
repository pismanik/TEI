      <choice>
       <reg>
        Уехал
       </reg>
       <orig>
        Уѣхалъ
       </orig>
      </choice>
      <choice original_editorial_correction="А[ндрей]">
       <sic>
        А
       </sic>
       <corr>
        Андрей
       </corr>
      </choice>
      <choice>
       <reg>
        с
       </reg>
       <orig>
        съ
       </orig>
      </choice>
      <choice original_editorial_correction="ж[еной]">
