     <p id="8e23b53e-5ae7-4a86-ac10-b8d51892c1a2">
      <choice>
       <reg>
        Приехал
       </reg>
       <orig>
        Прiѣхалъ
       </orig>
      </choice>
      милый
      <choice>
       <reg>
        Абрикосов
       </reg>
       <orig>
        Абрикосовъ
       </orig>
      </choice>
      <choice>
       <reg>
        с
       </reg>
       <orig>
        съ
       </orig>
      </choice>
